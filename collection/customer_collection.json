							"    let email=email_u+'@gmail.com'\r",
							"    \r",
							"    pm.environment.set(\"name\", name);\r",
							"    pm.environment.set(\"email\", email);\r",
							"    pm.environment.set(\"phone_number\", phone_w);\r",
